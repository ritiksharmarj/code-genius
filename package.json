{
  "name": "code-genius",
  "displayName": "Code Genius",
  "description": "Code with ai agents",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "code-genius.open",
        "title": "Open ChatGPT WebView"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "code-genius-view",
          "title": "ChatGPT",
          "icon": "./icons/chats.svg"
        }
      ]
    },
    "views": {
      "code-genius-view": [
        {
          "type": "webview",
          "id": "code-genius.webview",
          "name": "ChatGPT"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.90.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
